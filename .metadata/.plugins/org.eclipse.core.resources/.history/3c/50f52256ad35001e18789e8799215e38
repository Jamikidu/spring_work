package di_p;

public class Wheel {

	String wname;
	int wheels;
	
	public String getName() {
		return wname;
	}
	public void setName(String name) {
		this.wname = name;
	}
	public int getWheels() {
		return wheels;
	}
	public void setWheels(int wheels) {
		this.wheels = wheels;
	}
	
	@Override
	public String toString() {
		return "Wheel [wname=" + wname + ", wheels=" + wheels + "]";
	}
	
}
