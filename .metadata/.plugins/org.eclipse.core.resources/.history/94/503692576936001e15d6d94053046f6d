package di_p;

public class Calc {
	Student std;
}

class Student{
	String name;
	//int kscore,escore,mscore,sum,avg;
	int[] scores;
	int sum,avg;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int[] getScores() {
		return scores;
	}
	public void setScores(int[] scores) {
		this.scores = scores;
	}
	public int getSum() {
		return sum;
	}
	public void setSum(int[] scores) {
		this.sum = scores[0];
	}
	public int getAvg() {
		return avg;
	}
	public void setAvg(int avg) {
		this.avg = avg;
	}
	
	
	
	
}
